	<template>
		<groupId>org.agiso.tempel.tests</groupId>
		<templateId>javaProject</templateId>
		<version>1.0.0</version>

		<params>
			<param key="name" name="Project name">
				<fixed>true</fixed>
				<value>SampleProject</value>
			</param>
		</params>

		<references>
			<reference>
				<groupId>org.agiso.tempel.templates</groupId>
				<templateId>eclipse.project.java</templateId>
				<version>1.0.0</version>

				<params>
					<param key="name">
						<fixed>true</fixed>
						<value>${top.name}</value>
					</param>
				</params>
			</reference>
			<reference>
				<groupId>org.agiso.tempel.tests</groupId>
				<templateId>javaClass</templateId>
				<version>1.0.0</version>

				<params>
					<param key="srcDir">
						<fixed>true</fixed>
						<value>${top.name}/src/main/java</value>
					</param>
				</params>
			</reference>
		</references>
	</template>
	<template>
		<groupId>org.agiso.tempel.tests</groupId>
		<templateId>javaBundleProject</templateId>
		<version>1.0.0</version>

		<params>
			<param key="name" name="Project name">
				<fixed>true</fixed>
				<value>SampleBundleProject</value>
			</param>
		</params>

		<references>
			<reference>
				<groupId>org.agiso.tempel.templates</groupId>
				<templateId>abstract.mkdirs</templateId>
				<version>1.0.0</version>

				<resource>
					<target>bundles/${top.name}</target>
				</resource>
			</reference>

			<reference>
				<groupId>org.agiso.tempel.tests</groupId>
				<templateId>javaProject</templateId>
				<version>1.0.0</version>

				<params>
					<param key="name">
						<fixed>true</fixed>
						<value>${top.name}</value>
					</param>
				</params>

				<workdir>bundles/${top.name}</workdir>
			</reference>
		</references>
	</template>




